# Module for testing the notify mechanism correctness.
MODULE notify
TIMESCALE 1
SCOPE cell_1 (0, 0)
    PROCESS cell_1_process
        TIMESCALE 1
        TRANSCRIPTION(pippo_gene, pippo_mrna)
        TRANSLATION(pippo_mrna, 5*pippo_protein)
SCOPE cell_2 (0, 1)
    PROCESS cell_2_process
        TIMESCALE 1
        TRANSCRIPTION(pluto_gene, pluto_mrna)
        TRANSLATION(pluto_mrna, 3*pluto_protein)
DIFFUSION cell_1, cell_2, [pippo_protein, pluto_protein]



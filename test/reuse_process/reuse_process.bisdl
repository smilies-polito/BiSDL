# In this test module first_proc is defined once in cell_1 SCOPE,
# and then reused in cell_2.
MODULE reuse_process
TIMESCALE 1
SCOPE cell_1 (0, 0)
    PROCESS first_proc
        TIMESCALE 1
        TRANSCRIPTION(aa_gene, aa_mrna)
        TRANSLATION(aa_mrna, aa_protein)
    JUXTACRINE_SIGNAL aa_protein -> cell_2
SCOPE cell_2 (0, 1)
    PROCESS second_proc
        TIMESCALE 1
        ENZYMATIC_REACTION(aa_receptor_active_protein, [bb_protein], [cc_protein])
SCOPE cell_3 (0, 2)
    PROCESS first_proc
    JUXTACRINE_SIGNAL aa_protein -> cell_2